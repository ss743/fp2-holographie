\RequirePackage{ltxcmds}[2009/08/05]
\RequirePackage{ufcd-kvopt}[2012/03/16]
\define@key{UFCD}{config}{%
  \csname UFCD@ConfigState@\UFCD@ConfigState\endcsname{#1}%
}
\def\UFCD@ConfigState{Load}
\def\UFCD@ConfigQueue{}
\let\UFCD@ConfigDo\relax
\def\UFCD@ConfigState@Load#1{%
  \ufcdAddConfigOption{#1}%
  \edef\UFCD@ConfigQueue{%
    \UFCD@ConfigQueue
    \UFCD@ConfigDo{#1}%
  }%
}
\def\UFCD@ConfigState@#1{%
  \ufcdAddConfigOption{#1}%
  \ufcdConfigLoad{#1}%
  \ufcdConfigCall{#1}%
}
\def\UFCD@ConfigProcess{%
  \let\UFCD@ConfigState\ltx@empty
  \let\UFCD@ConfigProcess\relax
  \def\UFCD@ConfigDo##1{%
    \UFCD@ConfigState@{##1}%
  }%
  \UFCD@ConfigQueue
  \let\UFCD@ConfigQueue\ltx@empty
}
\def\UFCD@ConfigInput#1{%
  \edef\UFCD@RestoreAtCatcode{%
    \catcode64=\the\catcode64\relax
  }%
  \makeatother
  \input{#1}%
  \UFCD@RestoreAtCatcode
}
\newcommand*{\ufcdConfigLoad}[1]{%
  \@ifundefined{UFCD@Config@#1}{%
    \edef\UFCD@ConfigRestoreAtCode{%
      \catcode64=\the\catcode64\relax
    }%
    \makeatletter
    \IfFileExists{ufcd-config-#1.cfg}{%
      \UFCD@ConfigInput{ufcd-config-#1.cfg}%
      \@ifundefined{UFCD@Config@#1}{%
        \PackageError{ufcd-config}{%
          Configuration file `ufcd-config-#1.cfg'\MessageBreak
          does not call \string\ufcdConfigSetup{#1}{...}%
        }\@ehc
      }{}%
    }{%
      \PackageError{ufcd-config}{%
        Configuration file `ufcd-config-#1.cfg' is missing%
      }\@ehc
    }%
    \UFCD@ConfigRestoreAtCode
  }{%
  }%
}
\newcommand*{\ufcdConfigSetup}{%
  \@ifstar\UFCD@ConfigSetupNoFile\UFCD@ConfigSetup
}
\def\UFCD@ConfigSetupNoFile#1{%
  \expandafter\providecommand\expandafter*%
  \csname UFCD@ConfigOption@#1\endcsname{}%
  \UFCD@ConfigSetup{#1}%
}
\def\UFCD@ConfigSetup#1{%
  \@ifundefined{UFCD@Config@#1}{%
    \expandafter\def\csname UFCD@Config@#1\endcsname
  }{%
    \PackageError{ufcd-config}{%
      Configuration `#1' is already defined%
    }\@ehc
    \ltx@gobble
  }%
}
\def\ufcdConfigCall#1{%
  \begingroup\expandafter\expandafter\expandafter\endgroup
  \expandafter\ifx\csname UFCD@Config@#1\endcsname\relax
    \PackageError{UFCD@-config}{%
      Configuration `#1' does not exist%
    }\@ehc
  \else
    \csname UFCD@Config@#1\endcsname
  \fi
}
\newcommand*{\ufcdIfLetterClass}{%
  \begingroup\expandafter\expandafter\expandafter\endgroup
  \expandafter\ifx\csname ver@scrlttr2.cls\endcsname\relax
    \expandafter\ltx@secondoftwo
  \else
    \expandafter\ltx@firstoftwo
  \fi
}
\newcommand*{\ufcdAddConfigOption}[1]{%
  \@ifundefined{UFCD@ConfigOption@#1}{%
    \@ifundefined{KV@UFCD@#1}{%
      \edef\UFCD@ConfigRestoreAtCode{%
        \catcode64=\the\catcode64\relax
      }%
      \makeatletter
      \IfFileExists{ufcd-config-#1.cfg}{%
        \SetupKeyvalOptions{%
          family=UFCD,%
          prefix=UFCD@ConfigOption@,%
        }%
        \DeclareVoidOption{#1}{%
          \KV@UFCD@config{#1}%
        }%
        \PackageInfo{ufcd-config}{%
          Configuration option `#1' added%
        }%
      }{%
        \PackageWarningNoLine{ufcd-config}{%
          Cannot add configuration\MessageBreak
          option `#1',\MessageBreak
          because configuration file\MessageBreak
          `ufcd-config-#1.cfg'\MessageBreak
          is missing%
        }%
      }%
      \UFCD@ConfigRestoreAtCode
    }{%
      \PackageWarningNoLine{ufcd-config}{%
        Cannot add configuration option `#1',\MessageBreak
        because `#1' is already an option.\MessageBreak
        Use `config=#1' for loading the configuration%
      }%
    }%
  }{}%
}
