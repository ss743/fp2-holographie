\newcommand*{\ufcdProvideCaptionNameGermanEnglish}[4][\z@]{%
  \@for\UFCD@temp:=\ufcdLanguageListGerman\do{%
    \ufcdProvideCaptionName\UFCD@temp#2{#3}%
  }%
  \@for\UFCD@temp:=\ufcdLanguageListEnglish\do{%
    \ufcdProvideCaptionName\UFCD@temp#2{#4}%
  }%
  \ifcase#1\relax
  \or
    \providecommand*{#2}{#3}%
  \or
    \providecommand*{#2}{#4}%
  \fi
}
\newcommand*{\ufcdSetCaptionNameGermanEnglish}[4][\z@]{%
  \@for\UFCD@temp:=\ufcdLanguageListGerman\do{%
    \ufcdSetCaptionName\UFCD@temp#2{#3}%
  }%
  \@for\UFCD@temp:=\ufcdLanguageListEnglish\do{%
    \ufcdSetCaptionName\UFCD@temp#2{#4}%
  }%
  \ifcase#1\relax
  \or
    \def#2{#3}%
  \or
    \def#2{#4}%
  \fi
}
