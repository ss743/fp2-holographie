        \PackageInfo{ufcd-lang-load}{%
          Captions for `#1' are not available.\MessageBreak
          Ignoring this and further caption name settings\MessageBreak
          for this language%
        }%
      \fi
    }{}%
  \else
    \begingroup
      \def\UFCD@temp{\@empty}%
      \let#2\UFCD@temp
      \csname captions#1\endcsname
    \expandafter\endgroup
    \ifx#2\UFCD@temp
      \expandafter\g@addto@macro\csname captions#1\endcsname{%
        \def#2{#3}%
      }%
      \begingroup
        \edef\UFCD@temp{#1}%
        \@onelevel@sanitize\UFCD@temp
        \@onelevel@sanitize\languagename
      \expandafter\endgroup
      \ifx\UFCD@temp\languagename
        \def#2{#3}%
      \fi
    \else
      \ifUFCD@verbose
        \PackageInfo{ufcd-lang-load}{%
          \string\ufcdProvideCaptionName{#1}:\MessageBreak
          \string#2 is already present%
        }%
      \fi
    \fi
  \fi
}%
\newcommand*{\ufcdSetCaptionName}[3]{%
